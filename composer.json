{
  "name": "maiorano84/shortcodes",
  "description": "Implement Shortcode syntax anywhere",
  "keywords": ["wordpress", "shortcode", "library", "manager"],
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matt Maiorano",
      "email": "maiorano84@gmail.com",
      "homepage": "https://mattmaiorano.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.25",
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Maiorano\\Shortcodes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maiorano\\Shortcodes\\Test\\": "tests/"
    }
  },
  "scripts": {
    "phpunit": "phpunit --verbose --coverage-clover=clover.xml",
    "phpstan": "phpstan analyse src -l 7"
  }
}
